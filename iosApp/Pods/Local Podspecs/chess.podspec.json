{
  "name": "chess",
  "version": "1.0",
  "homepage": "Link to the Shared Module homepage",
  "source": {
    "git": "Not Published",
    "tag": "Cocoapods/chess/1.0"
  },
  "authors": "",
  "license": "",
  "summary": "Chess game referee: Defines models and rules used to coordinate gameplay",
  "static_framework": true,
  "vendored_frameworks": "build/cocoapods/framework/chess.framework",
  "libraries": "c++",
  "module_name": "chess_umbrella",
  "platforms": {
    "ios": "14.1"
  },
  "pod_target_xcconfig": {
    "KOTLIN_TARGET[sdk=iphonesimulator*]": "ios_x64",
    "KOTLIN_TARGET[sdk=iphoneos*]": "ios_arm",
    "KOTLIN_TARGET[sdk=watchsimulator*]": "watchos_x64",
    "KOTLIN_TARGET[sdk=watchos*]": "watchos_arm",
    "KOTLIN_TARGET[sdk=appletvsimulator*]": "tvos_x64",
    "KOTLIN_TARGET[sdk=appletvos*]": "tvos_arm64",
    "KOTLIN_TARGET[sdk=macosx*]": "macos_x64"
  },
  "script_phases": [
    {
      "name": "Build chess",
      "execution_position": "before_compile",
      "shell_path": "/bin/sh",
      "script": "                set -ev\n                REPO_ROOT=\"$PODS_TARGET_SRCROOT\"\n                \"$REPO_ROOT/../gradlew\" -p \"$REPO_ROOT\" :chess:syncFramework                     -Pkotlin.native.cocoapods.target=$KOTLIN_TARGET                     -Pkotlin.native.cocoapods.configuration=$CONFIGURATION                     -Pkotlin.native.cocoapods.cflags=\"$OTHER_CFLAGS\"                     -Pkotlin.native.cocoapods.paths.headers=\"$HEADER_SEARCH_PATHS\"                     -Pkotlin.native.cocoapods.paths.frameworks=\"$FRAMEWORK_SEARCH_PATHS\"\n"
    }
  ]
}
